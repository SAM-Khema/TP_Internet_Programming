{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'category',\n\n  data() {\n    return {\n      category_name: '',\n      new_category_name: '',\n      categories: [],\n      _id: ''\n    };\n  },\n\n  methods: {\n    redirect(path) {\n      this.$router.push(path);\n    },\n\n    open_category() {\n      document.getElementById('add').style.display = \"flex\";\n    },\n\n    close_category() {\n      document.getElementById('add').style.display = \"none\";\n    },\n\n    open_edit_category(id, name) {\n      document.getElementById('edit').style.display = \"flex\";\n      this.new_category_name = name;\n      this._id = id;\n    },\n\n    close_edit_category() {\n      document.getElementById('edit').style.display = \"none\";\n    },\n\n    add_new() {\n      const data = {\n        category_name: this.category_name\n      };\n      axios.post('http://localhost:3001/category/create', data).then(response => {\n        if (response.status === 200) {\n          alert('created successfully');\n        } else {\n          alert('create failed');\n        }\n\n        this.name = '';\n        this.close_category();\n        this.remount();\n      });\n    },\n\n    edit_new() {\n      const data = {\n        name: this.new_name,\n        _id: this._id\n      };\n      axios.put('http://localhost:3001/category/update', data).then(response => {\n        if (response.status === 200) {\n          alert('edit successfully');\n        } else {\n          alert('edit failled');\n        }\n\n        this.new_name = '';\n        this.close_edit_category();\n        this.remount();\n      });\n    },\n\n    delete_category(_id) {\n      const data = {\n        _id: _id\n      };\n      axios.delete('http://localhost:3001/category/delete/id', {\n        data: data\n      }).then(response => {\n        alert('delete successfully');\n        this.remount();\n      });\n    },\n\n    remount() {\n      axios.get('http://localhost:3001/category/get_categories').then(response => {\n        this.categories = response.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    axios.get('http://localhost:3000/category/get_categories').then(response => {\n      this.categories = response.data;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AA0LA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLC,aAAa,EAAE,EADV;MAELC,iBAAiB,EAAE,EAFd;MAGLC,UAAU,EAAE,EAHP;MAILC,GAAG,EAAE;IAJA,CAAP;EAMD,CATY;;EAUbC,OAAO,EAAE;IACHC,QAAQ,CAACC,IAAD,EAAM;MAChB,KAAKC,OAAL,CAAaC,IAAb,CAAkBF,IAAlB;IACF,CAHO;;IAKPG,aAAY,GAAI;MACdC,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAPM;;IAQPC,cAAa,GAAI;MACfJ,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAVM;;IAWPE,kBAAiB,CAAGC,EAAH,EAAOlB,IAAP,EAAY;MAC3BY,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;MACA,KAAKZ,iBAAL,GAAyBH,IAAzB;MACA,KAAKK,GAAL,GAAWa,EAAX;IACD,CAfM;;IAgBPC,mBAAkB,GAAI;MACpBP,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;IACD,CAlBM;;IAmBPK,OAAO,GAAE;MACP,MAAMnB,IAAG,GAAI;QAACC,aAAa,EAAE,KAAKA;MAArB,CAAb;MACAH,KAAK,CAACsB,IAAN,CAAW,uCAAX,EAAoDpB,IAApD,EACGqB,IADH,CACSC,QAAD,IAAc;QAClB,IAAGA,QAAQ,CAACC,MAAT,KAAoB,GAAvB,EAA2B;UACzBC,KAAK,CAAC,sBAAD,CAAL;QACD,CAFD,MAEK;UACHA,KAAK,CAAC,eAAD,CAAL;QACF;;QACA,KAAKzB,IAAL,GAAY,EAAZ;QACA,KAAKgB,cAAL;QACA,KAAKU,OAAL;MACD,CAVH;IAWD,CAhCM;;IAiCPC,QAAQ,GAAE;MACR,MAAM1B,IAAG,GAAI;QAACD,IAAI,EAAE,KAAK4B,QAAZ;QAAsBvB,GAAG,EAAE,KAAKA;MAAhC,CAAb;MACAN,KAAK,CAAC8B,GAAN,CAAU,uCAAV,EAAmD5B,IAAnD,EACCqB,IADD,CACOC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,KAAoB,GAAxB,EAA4B;UAC1BC,KAAK,CAAC,mBAAD,CAAL;QACD,CAFD,MAEK;UACHA,KAAK,CAAC,cAAD,CAAL;QACF;;QACA,KAAKG,QAAL,GAAgB,EAAhB;QACA,KAAKT,mBAAL;QACA,KAAKO,OAAL;MACD,CAVD;IAWD,CA9CM;;IA+CPI,eAAe,CAACzB,GAAD,EAAK;MAClB,MAAMJ,IAAG,GAAI;QAACI,GAAG,EAAEA;MAAN,CAAb;MACAN,KAAK,CAACgC,MAAN,CAAa,0CAAb,EAAyD;QAAC9B,IAAI,EAAEA;MAAP,CAAzD,EACGqB,IADH,CACSC,QAAD,IAAc;QAChBE,KAAK,CAAC,qBAAD,CAAL;QACA,KAAKC,OAAL;MACH,CAJH;IAKD,CAtDM;;IAuDPA,OAAO,GAAE;MACP3B,KAAK,CAACiC,GAAN,CAAU,+CAAV,EACGV,IADH,CACSC,QAAD,IAAc;QAClB,KAAKnB,UAAL,GAAkBmB,QAAQ,CAACtB,IAA3B;MACD,CAHH;IAIF;;EA5DO,CAVI;;EAwEbgC,OAAO,GAAE;IACLlC,KAAK,CAACiC,GAAN,CAAU,+CAAV,EACGV,IADH,CACSC,QAAD,IAAc;MAClB,KAAKnB,UAAL,GAAkBmB,QAAQ,CAACtB,IAA3B;IACD,CAHH;EAIJ;;AA7Ea,CAAf","names":["axios","name","data","category_name","new_category_name","categories","_id","methods","redirect","path","$router","push","open_category","document","getElementById","style","display","close_category","open_edit_category","id","close_edit_category","add_new","post","then","response","status","alert","remount","edit_new","new_name","put","delete_category","delete","get","mounted"],"sourceRoot":"","sources":["D:\\Assignment document of I4\\Internet programing\\tp12\\TP12\\ex02-topone-app\\src\\views\\Category.vue"],"sourcesContent":["<template>\n  <header>\n    <p>Admin dashboard</p>\n    <p>លេខមួយ.com</p>\n  </header>\n  <main>\n    <div class=\"left-bar\">\n      <li @click=\"redirect('/category') \" >Category</li>\n      <li @click=\"redirect('/subcategory')\">Subcategory</li>\n      <li @click=\"redirect('/product')\" >Product</li>\n    </div>\n    <div class=\"right-side\">\n      <div class=\"action\">\n        <p>Category</p>\n        <button @click=\"open_category\" style=\"margin-top: 5px;border-radius: 7px;margin-right: 10px;background-color: burlywood; \">Add new</button>\n      </div>\n      <!-- HTML Code: Place this code in the document's body (between the 'body' tags) where the table should appear -->\n      <table class=\"GeneratedTable\">\n        <thead>\n          <tr>\n            <th>Name</th>\n            <th>Description</th>\n            <th>Image url</th>\n            <th>Action</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"category in categories\" :key=\"category._id\">\n            <td>{{category.name}}</td>\n            <td></td>\n            <td></td>\n            <td>\n              <button @click=\"delete_category(category._id)\">Delete</button>\n              <button @click=\"open_edit_category(category._id, category.name)\">Edit</button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n      <!-- Codes by Quackit.com -->\n    </div>\n    <div class=\"adding\" id=\"add\">\n      <h2>Add new Category</h2>\n      <div class=\"input-field\">\n        <input v-model=\"name\" type=\"text\" placeholder=\"Category\">\n        <!-- <input type=\"text\" placeholder=\"Description\">\n        <input type=\"text\" placeholder=\"Price\"> -->\n      </div>\n      <div class=\"button-container\">\n        <button @click=\"close_category\">Cancel</button>\n        <button @click=\"add_new\">Create</button>\n      </div>\n    </div>\n    <div class=\"adding\" id=\"edit\">\n      <h2>Add new Category</h2>\n      <div class=\"input-field\">\n        <input v-model=\"new_name\" type=\"text\" placeholder=\"New Category\">\n      </div>\n      <div class=\"button-container\">\n        <button @click=\"close_edit_category\">Cancel</button>\n        <button @click=\"edit_new\">Create</button>\n      </div>\n    </div>\n  </main>\n</template>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nli{\n  background-color:  rgb(199, 151, 61);\n}\nli:active{\n  background-color: rgb(214, 175, 103);\n}\n\nheader {\n  width: 100%;\n  height: 80px;\n  border-bottom: 2px solid black;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 32px;\n  position: fixed;\n  z-index: 999;\n  padding: 0 40px;\n  background-color:burlywood;\n}\n\nmain {\n  position: fixed;\n  top: 80px;\n  display: flex;\n}\n\nmain .left-bar {\n  width: 300px;\n  border-right: 2px solid black;\n  height: calc(100vh - 80px);\n  \n}\n\nmain .left-bar li {\n  list-style: none;\n  border-bottom: 1px solid black;\n  padding: 10px;\n  text-align: center;\n}\n\nmain .right-side {\n  width: calc(100vw - 300px);\n  margin-bottom: 20px;\n}\n\nmain .right-side .action {\n  display: flex;\n  justify-content: space-between;\n}\n\nmain .right-side .action p {\n  font-size: 18px;\n}\n\nmain .right-side .action button {\n  width: 120px;\n  height: 30px;\n  border: 1px solid black;\n}\n\nmain .right-side .GeneratedTable{\n  width: 100%;\n  padding: 0 20px;\n}\n\nmain .right-side .GeneratedTable th{\n  text-align: start;\n  font-weight: 700;\n}\n\nmain .right-side .GeneratedTable button{\n  width: 90px;\n  border: none;\n  margin-right: 10px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\nmain .adding{\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%,-80%);\n  display: none;\n  flex-direction: column;\n  padding: 20px;\n  width: 400px;\n  box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;\n  background: #fff;\n  text-align: center;\n  gap: 40px;\n  z-index: 999;\n}\n\nmain .adding .input-field{\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\nmain .adding .input-field input{\n  padding: 0 10px;\n}\n\nmain .adding .button-container{\n  display: flex;\n  gap: 10px;\n  justify-content: flex-end;\n}\n\n</style>\n\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'category',\n  data(){\n    return {\n      category_name: '',\n      new_category_name: '',\n      categories: [],\n      _id: ''\n    }\n  },\n  methods: {\n        redirect(path){\n      this.$router.push(path)\n    }\n    ,\n    open_category (){\n      document.getElementById('add').style.display = \"flex\"\n    },\n    close_category (){\n      document.getElementById('add').style.display = \"none\"\n    },\n    open_edit_category (id, name){\n      document.getElementById('edit').style.display = \"flex\"\n      this.new_category_name = name\n      this._id = id\n    },\n    close_edit_category (){\n      document.getElementById('edit').style.display = \"none\"\n    },\n    add_new(){\n      const data = {category_name: this.category_name}\n      axios.post('http://localhost:3001/category/create', data)\n        .then((response) => {\n          if(response.status === 200){\n            alert('created successfully')\n          }else{\n            alert('create failed')\n          }\n          this.name = ''\n          this.close_category()\n          this.remount()\n        })\n    },\n    edit_new(){\n      const data = {name: this.new_name, _id: this._id}\n      axios.put('http://localhost:3001/category/update', data)\n      .then((response) => {\n        if (response.status === 200){\n          alert('edit successfully')\n        }else{\n          alert('edit failled')\n        }\n        this.new_name = ''\n        this.close_edit_category()\n        this.remount()\n      })\n    },\n    delete_category(_id){\n      const data = {_id: _id}\n      axios.delete('http://localhost:3001/category/delete/id', {data: data})\n        .then((response) => {\n            alert('delete successfully')\n            this.remount()\n        })\n    },\n    remount(){\n      axios.get('http://localhost:3001/category/get_categories')\n        .then((response) => {\n          this.categories = response.data\n        })\n    }\n  },\n  mounted(){\n      axios.get('http://localhost:3000/category/get_categories')\n        .then((response) => {\n          this.categories = response.data\n        })\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}