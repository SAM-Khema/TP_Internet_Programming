{"ast":null,"code":"// import axios from 'axios'\nimport categoryApi from '../libs/apis/category';\nimport productsApi from '../libs/apis/product';\nexport default {\n  name: 'category',\n\n  data() {\n    return {\n      name: '',\n      new_category_name: '',\n      categories: [],\n      _id: ''\n    };\n  },\n\n  async mounted() {\n    this.categories = await categoryApi.getCategoryItem();\n    this.products = await productsApi.getAllProduct(\"\", \"\");\n    console.log(this.categories);\n  },\n\n  methods: {\n    async onClick(categoryId, itemId) {\n      this.products = await productsApi.getAllProduct(categoryId, itemId);\n    }\n\n  },\n  methods: {\n    redirect(path) {\n      this.$router.push(path);\n    },\n\n    open_category() {\n      document.getElementById('add').style.display = \"flex\";\n    },\n\n    close_category() {\n      document.getElementById('add').style.display = \"none\";\n    },\n\n    open_edit_category(id, name) {\n      document.getElementById('edit').style.display = \"flex\";\n      this.new_category_name = name;\n      this._id = id;\n    },\n\n    close_edit_category() {\n      document.getElementById('edit').style.display = \"none\";\n    },\n\n    add_new() {\n      const data = {\n        category_name: this.category_name\n      };\n      axios.post('http://localhost:3001/category/create', data).then(response => {\n        if (response.status === 200) {\n          alert('created successfully');\n        } else {\n          alert('create failed');\n        }\n\n        this.name = '';\n        this.close_category();\n        this.remount();\n      });\n    },\n\n    edit_new() {\n      const data = {\n        name: this.new_name,\n        _id: this._id\n      };\n      axios.put('http://localhost:3001/category/update', data).then(response => {\n        if (response.status === 200) {\n          alert('edit successfully');\n        } else {\n          alert('edit failled');\n        }\n\n        this.new_name = '';\n        this.close_edit_category();\n        this.remount();\n      });\n    },\n\n    delete_category(_id) {\n      const data = {\n        _id: _id\n      };\n      axios.delete('http://localhost:3001/category/delete/id', {\n        data: data\n      }).then(response => {\n        alert('delete successfully');\n        this.remount();\n      });\n    },\n\n    remount() {\n      axios.get('http://localhost:3001/category/all').then(response => {\n        this.categories = response.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    axios.get('http://localhost:3001/category/all').then(response => {\n      this.categories = response.data;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AA0LA;AACA,OAAOA,WAAP,MAAwB,uBAAxB;AACA,OAAOC,WAAP,MAAwB,sBAAxB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLD,IAAI,EAAE,EADD;MAELE,iBAAiB,EAAE,EAFd;MAGLC,UAAU,EAAE,EAHP;MAILC,GAAG,EAAE;IAJA,CAAP;EAMD,CATY;;EAUb,MAAMC,OAAN,GAAgB;IACF,KAAKF,UAAL,GAAkB,MAAML,WAAW,CAACQ,eAAZ,EAAxB;IACA,KAAKC,QAAL,GAAgB,MAAMR,WAAW,CAACS,aAAZ,CAA0B,EAA1B,EAA8B,EAA9B,CAAtB;IACAC,OAAO,CAACC,GAAR,CAAY,KAAKP,UAAjB;EAEP,CAfM;;EAgBPQ,OAAO,EAAE;IACD,MAAMC,OAAN,CAAcC,UAAd,EAA0BC,MAA1B,EAAkC;MAE1B,KAAKP,QAAL,GAAgB,MAAMR,WAAW,CAACS,aAAZ,CAA0BK,UAA1B,EAAsCC,MAAtC,CAAtB;IACP;;EAJA,CAhBF;EAwBbH,OAAO,EAAE;IACHI,QAAQ,CAACC,IAAD,EAAM;MAChB,KAAKC,OAAL,CAAaC,IAAb,CAAkBF,IAAlB;IACF,CAHO;;IAKPG,aAAY,GAAI;MACdC,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAPM;;IAQPC,cAAa,GAAI;MACfJ,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAVM;;IAWPE,kBAAiB,CAAGC,EAAH,EAAO1B,IAAP,EAAY;MAC3BoB,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;MACA,KAAKrB,iBAAL,GAAyBF,IAAzB;MACA,KAAKI,GAAL,GAAWsB,EAAX;IACD,CAfM;;IAgBPC,mBAAkB,GAAI;MACpBP,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;IACD,CAlBM;;IAmBPK,OAAO,GAAE;MACP,MAAM3B,IAAG,GAAI;QAAC4B,aAAa,EAAE,KAAKA;MAArB,CAAb;MACAC,KAAK,CAACC,IAAN,CAAW,uCAAX,EAAoD9B,IAApD,EACG+B,IADH,CACSC,QAAD,IAAc;QAClB