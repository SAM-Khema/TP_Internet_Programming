{"ast":null,"code":"import categoryApi from '../libs/ex01_crud/category';\nimport productsApi from '../libs/ex01_crud/product'; // import axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      categories: [],\n      items: [],\n      products: [],\n      selected_tab: 'Category'\n    };\n  },\n\n  async mounted() {\n    this.categories = await categoryApi.getCategoryItem();\n    this.products = await productsApi.getAllProduct(\"\", \"\");\n    console.log(this.categories);\n  },\n\n  methods: {\n    async onClick(categoryId, itemId) {\n      this.products = await productsApi.getAllProduct(categoryId, itemId);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4EA,OAAOA,WAAP,MAAwB,4BAAxB;AACA,OAAOC,WAAP,MAAwB,2BAAxB,C,CAEA;;AAEA,eAAe;EACPC,IAAI,GAAG;IACC,OAAO;MACCC,UAAU,EAAE,EADb;MAECC,KAAK,EAAE,EAFR;MAGCC,QAAQ,EAAE,EAHX;MAICC,YAAY,EAAE;IAJf,CAAP;EAMP,CARM;;EAUP,MAAMC,OAAN,GAAgB;IACR,KAAKJ,UAAL,GAAkB,MAAMH,WAAW,CAACQ,eAAZ,EAAxB;IACA,KAAKH,QAAL,GAAgB,MAAMJ,WAAW,CAACQ,aAAZ,CAA0B,EAA1B,EAA8B,EAA9B,CAAtB;IACAC,OAAO,CAACC,GAAR,CAAY,KAAKR,UAAjB;EAEP,CAfM;;EAgBPS,OAAO,EAAE;IACD,MAAMC,OAAN,CAAcC,UAAd,EAA0BC,MAA1B,EAAkC;MAE1B,KAAKV,QAAL,GAAgB,MAAMJ,WAAW,CAACQ,aAAZ,CAA0BK,UAA1B,EAAsCC,MAAtC,CAAtB;IACP;;EAJA;AAhBF,CAAf","names":["categoryApi","productsApi","data","categories","items","products","selected_tab","mounted","getCategoryItem","getAllProduct","console","log","methods","onClick","categoryId","itemId"],"sourceRoot":"","sources":["D:\\Assignment document of I4\\Internet programing\\tp12\\TP12\\ex02-topone-app\\src\\views\\AdminView.vue"],"sourcesContent":["<template>\r\n        <div class=\"border border-dark\">\r\n                <div class=\"d-flex justify-content-between p-2\">\r\n                        <h1>Admin Dashboard</h1>\r\n                        <h1 class=\"mx-5\">លេខ១.com</h1>\r\n                </div>\r\n                <div class=\"border border-dark d-flex\">\r\n                        <div class=\"sidebar\">\r\n                                <div class=\"bar\" @click=\"selected_tab = 'Category'\">Category</div>\r\n                                <div class=\"bar\" @click=\"selected_tab = 'Items'\">Items</div>\r\n                                <div class=\"bar\" @click=\"selected_tab = 'Products'\">Product</div>\r\n                        </div>\r\n\r\n                        <div class=\"inforn \">\r\n                                <div class=\"d-flex justify-content-between\">\r\n                                        <h1 class=\"m-3\">{{ selected_tab }}</h1>\r\n                                        <!-- add new -->\r\n                                        <button class=\"add m-3 shadow-inner\" color=\"primary\"  > Add new </button>\r\n                                       \r\n                                </div>\r\n\r\n                                <div class=\"border border-dark w-75 m-auto mt-5 p-2\">\r\n                                        <div class=\"container\">\r\n                                                <div class=\"row\">\r\n                                                        <p class=\"col\">Name</p>\r\n                                                        <p class=\"col\">description</p>\r\n                                                        <p class=\"col \">ImageUrl</p>\r\n                                                        <p class=\"col\">Action</p>\r\n                                                </div>\r\n                                                <!-- categoery -->\r\n                                                <div class=\"border-top border-dark category \" v-if=\"selected_tab == 'Category'\">\r\n                                                        <div class=\"row child \" v-for=\"category in categories\"\r\n                                                                :key=\"category._id\">\r\n                                                                <p class=\"col \">{{ category.name }} </p>\r\n                                                                <p class=\"col \">{{ category.desc }}</p>\r\n                                                                <p class=\"col text-truncate \">{{ category.imageUrl }}</p>\r\n                                                                <p class=\"col \">edit/remove</p>\r\n                                                        </div>\r\n                                                </div>\r\n                                                <!-- item -->\r\n                                                <div class=\"border-top border-dark items\" v-if=\"selected_tab == 'Items'\">\r\n                                                        <div class=\"row\" v-for=\"category in categories\">\r\n                                                                <div class=\"row child\" v-for=\"item in category?.items\"\r\n                                                                        :key=\"item._id\">\r\n                                                                        <p class=\"col \">{{ item.name }} </p>\r\n                                                                        <p class=\"col \">{{ item.desc }}</p>\r\n                                                                        <p class=\"col text-truncate \">{{ item.imageUrl }}</p>\r\n                                                                        <p class=\"col \">edit/remove</p>\r\n                                                                </div>\r\n                                                        </div>\r\n                                                </div>\r\n                                                <!-- Product -->\r\n                                                <div class=\"border-top border-dark product\" v-if=\"selected_tab == 'Products'\">\r\n                                                        <div class=\"row \" v-for=\"category in categories\">\r\n                                                                <div class=\"row \" v-for=\"item in category?.items\">\r\n                                                                        <div class=\"row child\" v-for=\"product in item?.products\"\r\n                                                                                :key=\"product._id\">\r\n                                                                                <p class=\"col \">{{ product.title }} </p>\r\n                                                                                <p class=\"col \">{{ product.desc }}</p>\r\n                                                                                <p class=\"col text-truncate \">{{\r\n                                                                                        product.imageUrl }}</p>\r\n                                                                                <p class=\"col \">edit/remove</p>\r\n                                                                        </div>\r\n                                                                </div>\r\n                                                        </div>\r\n                                                </div>\r\n                                        </div>\r\n                                </div>\r\n\r\n\r\n                        </div>\r\n\r\n                </div>\r\n        </div>\r\n</template>\r\n<script>\r\nimport categoryApi from '../libs/ex01_crud/category';\r\nimport productsApi from '../libs/ex01_crud/product';\r\n\r\n// import axios from 'axios'\r\n\r\nexport default {\r\n        data() {\r\n                return {\r\n                        categories: [],\r\n                        items: [],\r\n                        products: [],\r\n                        selected_tab: 'Category',\r\n                }\r\n        },\r\n\r\n        async mounted() {\r\n                this.categories = await categoryApi.getCategoryItem();\r\n                this.products = await productsApi.getAllProduct(\"\", \"\");\r\n                console.log(this.categories);\r\n\r\n        },\r\n        methods: {\r\n                async onClick(categoryId, itemId) {\r\n\r\n                        this.products = await productsApi.getAllProduct(categoryId, itemId)\r\n                },\r\n\r\n\r\n        }\r\n}\r\n</script>\r\n\r\n<style>\r\n.sidebar {\r\n        width: 20%;\r\n        height: 700px;\r\n        /* background-color:rgb(214 211 209); */\r\n}\r\n\r\n.bar {\r\n        display: block;\r\n        padding: 5px;\r\n        text-align: center;\r\n        border-bottom: 1px solid black;\r\n        cursor: pointer;\r\n}\r\n\r\n.bar:hover {\r\n        background-color: rgb(186 230 253);\r\n}\r\n\r\n.add:hover {\r\n        background-color: rgb(171, 171, 198);\r\n}\r\n\r\n.inforn {\r\n        width: 80%;\r\n        border-left: 1px solid black;\r\n        /* background-color:  rgb(186 230 253); */\r\n}\r\n\r\n.add {\r\n        width: 90px;\r\n        height: 30px;\r\n        border-radius: 10px;\r\n        /* margin: 25px; */\r\n}\r\n\r\n.child:nth-child(2n) {\r\n        margin: 5px;\r\n}\r\n\r\n.child:nth-child(2n-1) {\r\n\r\n        background-color: rgb(231, 237, 237);\r\n        margin: 5px;\r\n}</style>"]},"metadata":{},"sourceType":"module"}