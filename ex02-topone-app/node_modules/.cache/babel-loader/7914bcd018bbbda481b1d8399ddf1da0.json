{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'category',\n\n  data() {\n    return {\n      subcategory_name: '',\n      category_id: '',\n      category_name: '',\n      new_subcategory_name: '',\n      subcategories: [],\n      _id: ''\n    };\n  },\n\n  methods: {\n    redirect(path) {\n      this.$router.push(path);\n    },\n\n    open_subcategory() {\n      document.getElementById('add').style.display = \"flex\";\n    },\n\n    close_subcategory() {\n      document.getElementById('add').style.display = \"none\";\n    },\n\n    open_edit_subcategory(id, name) {\n      document.getElementById('edit').style.display = \"flex\";\n      this.new_subcategory_name = name;\n      this._id = id;\n    },\n\n    close_edit_subcategory() {\n      document.getElementById('edit').style.display = \"none\";\n    },\n\n    add_new() {\n      const data = {\n        subcategory_name: this.subcategory_name,\n        category_id: this.category_id\n      };\n      axios.post('http://localhost:3000/subcategory/create_subcategory', data).then(response => {\n        if (response.status === 200) {\n          alert('created successfully');\n        } else {\n          alert('create failed');\n        }\n\n        this.category_name = '';\n        this.close_subcategory();\n        this.remount();\n      });\n    },\n\n    edit_new() {\n      const data = {\n        subcategory_name: this.new_subcategory_name,\n        _id: this._id\n      };\n      axios.put('http://localhost:3000/subcategory/update_subcategory', data).then(response => {\n        if (response.status === 200) {\n          alert('edit successfully');\n        } else {\n          alert('edit failled');\n        }\n\n        this.new_subcategory_name = '';\n        this.close_edit_subcategory();\n        this.remount();\n      });\n    },\n\n    delete_subcategory(_id) {\n      const data = {\n        _id: _id\n      };\n      axios.delete('http://localhost:3000/subcategory/delete_subcategory', {\n        data: data\n      }).then(response => {\n        alert('delete successfully');\n        this.remount();\n      });\n    },\n\n    remount() {\n      axios.get('http://localhost:3000/subcategory/get_subcategories').then(response => {\n        this.subcategories = response.data;\n      });\n    }\n\n  },\n\n  mounted() {\n    axios.get('http://localhost:3000/subcategory/get_subcategories').then(response => {\n      this.subcategories = response.data;\n      console.log(response.data);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAyLA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MAELC,gBAAgB,EAAE,EAFb;MAGLC,WAAW,EAAE,EAHR;MAKLC,aAAa,EAAE,EALV;MAMLC,oBAAoB,EAAE,EANjB;MAOLC,aAAa,EAAE,EAPV;MAQLC,GAAG,EAAE;IARA,CAAP;EAUD,CAbY;;EAcbC,OAAO,EAAE;IACHC,QAAQ,CAACC,IAAD,EAAM;MAChB,KAAKC,OAAL,CAAaC,IAAb,CAAkBF,IAAlB;IACF,CAHO;;IAKPG,gBAAe,GAAI;MACjBC,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAPM;;IAQPC,iBAAgB,GAAI;MAClBJ,QAAQ,CAACC,cAAT,CAAwB,KAAxB,EAA+BC,KAA/B,CAAqCC,OAArC,GAA+C,MAA/C;IACD,CAVM;;IAWPE,qBAAoB,CAAGC,EAAH,EAAOpB,IAAP,EAAY;MAC9Bc,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;MACA,KAAKZ,oBAAL,GAA4BL,IAA5B;MACA,KAAKO,GAAL,GAAWa,EAAX;IACD,CAfM;;IAgBPC,sBAAqB,GAAI;MACvBP,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAAhC,CAAsCC,OAAtC,GAAgD,MAAhD;IACD,CAlBM;;IAmBPK,OAAO,GAAE;MACP,MAAMrB,IAAG,GAAI;QAACC,gBAAgB,EAAE,KAAKA,gBAAxB;QAA0CC,WAAW,EAAE,KAAKA;MAA5D,CAAb;MACAJ,KAAK,CAACwB,IAAN,CAAW,sDAAX,EAAmEtB,IAAnE,EACGuB,IADH,CACSC,QAAD,IAAc;QAClB,IAAGA,QAAQ,CAACC,MAAT,KAAoB,GAAvB,EAA2B;UACzBC,KAAK,CAAC,sBAAD,CAAL;QACD,CAFD,MAEK;UACHA,KAAK,CAAC,eAAD,CAAL;QACF;;QACA,KAAKvB,aAAL,GAAqB,EAArB;QACA,KAAKc,iBAAL;QACA,KAAKU,OAAL;MACD,CAVH;IAWD,CAhCM;;IAiCPC,QAAQ,GAAE;MACR,MAAM5B,IAAG,GAAI;QAACC,gBAAgB,EAAE,KAAKG,oBAAxB;QAA8CE,GAAG,EAAE,KAAKA;MAAxD,CAAb;MACAR,KAAK,CAAC+B,GAAN,CAAU,sDAAV,EAAkE7B,IAAlE,EACCuB,IADD,CACOC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,KAAoB,GAAxB,EAA4B;UAC1BC,KAAK,CAAC,mBAA